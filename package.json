{
  "name": "angular-material-app",
  "description": "基于最新Angular 5框架与Material 2技术的企业级通用管理系统快速开发框架，涵盖了大量的常用组件和基础功能，您可以快速开发各种MIS系统，如CMS、OA、CRM、ERP、POS等，最大程度上帮助企业节省时间成本和费用开支。",
  "homepage": "https://github.com/stbui/angular-material-app",
  "bugs": "https://github.com/stbui/angular-material-app/issues",
  "version": "0.5.2",
  "license": "MIT",
  "main": "dist/electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --extract-css",
    "start:proxy": "ng serve --extract-css --proxy-config proxy.conf.js",
    "build": "ng build --extract-css --prod --aot --environment=prod",
    "build:github": "ng build --extract-css --prod --aot --environment=github --base-href=/angular-material-app/",
    "publish:github": "ngh",
    "ngsw-config": "./node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build-prod-ngsw": "ng build --extract-css --prod --aot --environment=ngsw && npm run ngsw-config && npm run ngsw-copy",
    "build-github-ngsw": "npm run build:github && npm run ngsw-config && npm run ngsw-copy",
    "start:electron": "electron .",
    "electron-copy": "cp ./scripts/electron/electron.js dist/",
    "build-prod-electron": "ng build --extract-css --prod --aot --environment=prod --base-href=./ && npm run electron-copy",
    "build-pack-electron": "node ./scripts/electron/build.js",
    "docs": "hads ./docs -o",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/cdk": "^5.0.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/material": "^5.0.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@angular/service-worker": "^5.1.2",
    "@swimlane/ngx-charts": "^7.0.1",
    "@swimlane/ngx-datatable": "^11.1.5",
    "@swimlane/ngx-dnd": "^3.1.0",
    "angular-calendar": "^0.22.1",
    "angularfire2": "^5.0.0-rc.4",
    "core-js": "^2.5.3",
    "d3": "^4.12.0",
    "electron": "^1.7.10",
    "electron-packager": "^10.1.0",
    "firebase": "^4.8.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ng2-charts": "^1.6.0",
    "ngx-perfect-scrollbar": "^5.0.5",
    "ngx-quill": "^2.0.4",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.5.5",
    "screenfull": "^3.3.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.2",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@compodoc/compodoc": "^1.0.5",
    "@types/node": "^6.0.94",
    "codelyzer": "^3.2.2",
    "hads": "^1.6.0",
    "protractor": "^5.1.2",
    "ts-node": "^3.2.2",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  }
}
