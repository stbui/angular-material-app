{
  "name": "angular-material-app",
  "version": "0.4.5",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:themes && ng serve --extract-css",
    "start:proxy": "ng serve --extract-css --proxy-config proxy.conf.js",
    "build": "ng build --extract-css --prod --aot --environment=prod",
    "build:github": "ng build --extract-css --prod --aot --environment=github --base-href=/angular-material-app/",
    "publish:github": "ngh",
    "docs": "hads ./docs -o",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:theme:deeppurple": "$(npm bin)/node-sass src/assets/custom-themes/deeppurple-amber.scss src/assets/deeppurple-amber.css",
    "build:theme:indigo": "$(npm bin)/node-sass src/assets/custom-themes/indigo-pink.scss src/assets/indigo-pink.css",
    "build:theme:pink": "$(npm bin)/node-sass src/assets/custom-themes/pink-bluegrey.scss src/assets/pink-bluegrey.css",
    "build:theme:purple": "$(npm bin)/node-sass src/assets/custom-themes/purple-green.scss src/assets/purple-green.css",
    "build:themes": "npm run build:theme:deeppurple && npm run build:theme:indigo && npm run build:theme:pink && npm run build:theme:purple"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/flex-layout": "^2.0.0-beta.11",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@swimlane/ngx-charts": "^6.1.0",
    "@swimlane/ngx-dnd": "^3.1.0",
    "angular-calendar": "^0.21.3",
    "angularfire2": "^5.0.0-rc.3",
    "core-js": "^2.4.1",
    "d3": "^4.11.0",
    "firebase": "^4.6.0",
    "hammerjs": "^2.0.8",
    "markdown-it": "^8.4.0",
    "material-design-icons": "^3.0.1",
    "ng2-charts": "^1.6.0",
    "ngx-perfect-scrollbar": "^4.5.3",
    "ngx-quill": "^1.5.1",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.5.2",
    "screenfull": "^3.3.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.9",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.15",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "hads": "^1.3.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
